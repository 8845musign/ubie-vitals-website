{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@icons/*": ["src/icons/*"],
      "@layouts/*": ["src/layouts/*"],
      "@styles/*": ["src/styles/*"],
      "@utils/*": ["src/utils/*"],
      "@types": ["src/types/index.ts"],
      "@types/*": ["src/types/*"],
      "@constants": ["constants.ts"],
      "@sharedDocuments/*": ["src/sharedDocuments/*"],
      "@store/*": ["src/store/*"],
    },
    "types": [
      "vitest/importMeta"
    ]
  },
  "paths": { "*": ["@types/*"]}
}